<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@drawable/fddddd"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ru.setebe.fadicollection.Main3Activity">



    <WebView
        android:id="@+id/Websayt"
        android:layout_width="1dp"
        android:layout_height="1dp">
    </WebView>


    <RelativeLayout
        android:paddingBottom="5dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:layout_marginTop="250dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">





    <EditText

        android:textSize="20sp"
        android:hint="Tekefon raqam"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:id="@+id/ismga"
        android:text="+998"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="NotSibling" />



        <TextView
            android:id="@+id/soziz"
          android:layout_below="@id/telga"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:textSize="13sp"
            android:text="Buyurtmalar nomi" />



        <EditText

            android:layout_below="@+id/ismga"
            android:id="@+id/telga"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:gravity="center"
            android:hint="Ism"
            android:textSize="20sp" />


        <Button

            android:id="@+id/telegram"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="#f56"
            android:text="Malumotlarni yuborish"
            android:textColor="#ffff"
            android:textSize="10sp" />

    <Button
        android:background="#f56"
        android:textColor="#ffff"

        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:id="@+id/te"
        android:visibility="invisible"
        android:textSize="10sp"
        android:text="Location yuborish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_below="@+id/soziz"
        android:layout_centerHorizontal="true"
        android:id="@+id/soz"

        android:gravity="left"
        android:text=""
        android:textSize="15sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />





    </RelativeLayout>

</RelativeLayout>
